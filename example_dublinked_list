from flatten_dublinked_lists import Node, flatten, print_new_node_list

# Node example

head = Node(1)
node2 = Node(2)
node3 = Node(3)
node4 = Node(4)
node5 = Node(5)
node6 = Node(6)
child1 = Node(7)
child2 = Node(8)
child3 = Node(9)
child4 = Node(10)

head.next = node2
node2.prev = head
node2.next = node3
node3.prev = node2
node3.next = node4
node4.prev = node3
node4.next = node5
node5.prev = node4
node5.next = node6
node6.prev = node5

node3.child = child1
child1.next = child2
child2.prev = child1
child2.child = child3
child3.next = child4
child4.prev = child3



flattened_head = flatten(head)
print_new_node_list(flattened_head)